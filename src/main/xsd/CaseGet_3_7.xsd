							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:choice>
										<xs:annotation>
											<xs:documentation>Contains the identifier that was submitted with the request.</xs:documentation>
										</xs:annotation>
										<xs:element ref="CourtFileNumber"/>
										<xs:element ref="WarrantNumber"/>
										<xs:element name="ControlNumber">
											<xs:complexType>
												<xs:choice>
													<xs:sequence>
														<xs:element ref="ReportingAgencyORI"/>
														<xs:element ref="ReportingAgencyControlNumber"/>
													</xs:sequence>
													<xs:sequence>
														<xs:element ref="ArrestingAgencyORI"/>
