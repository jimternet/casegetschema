	12/27/2013		3:6				4				Made IssuingMethod optional. Change ChargeAmended to not be unbounded.   Fixed Charge within Citation to be unbounded.
	05/20/2015		3:7				1				Changed Bond to use the new WarrantBondType with new BondType element and repeating BondTypeText
																Changed SplitBond to use the new WarrantSplitType with new BondType element and repeating BondTypeText and to allow repeating SplitBonds
																Changed CaseFileDateTime to CaseFileDate
																Changed CaseEventDateTime to CaseEventDate and added a new optional element for CaseEventTime
																Added Amount to CourtCaseEvent
																Added Financial Detail (CaseOption and CaseInformation)
																Changed PersonPhysicalDetails within Citee from local elements to use the PersonPhysicalDetailsType
-->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.courts.state.mn.us/CourtXML/3" targetNamespace="http://www.courts.state.mn.us/CourtXML/3" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1">
	<xs:include schemaLocation="CourtXML_3_2.xsd"/>
	<xs:complexType name="CaseGetChargeType">
		<xs:sequence>
			<xs:element ref="ChargeSequenceID"/>
			<xs:element ref="ChargeFiledDate" minOccurs="0"/>
			<xs:element ref="OffenseID"/>
			<xs:element ref="OffenseCite" minOccurs="0"/>
			<xs:element ref="OffenseDescriptionText" minOccurs="0"/>
			<xs:element ref="OffenseDate"/>
			<xs:element ref="OffenseTime" minOccurs="0"/>
			<xs:element ref="OffenseDateEnd" minOccurs="0"/>
			<xs:element ref="OffenseTimeEnd" minOccurs="0"/>
			<xs:element ref="OffenseDatesOnOrAbout"/>
			<xs:element ref="OffenseTimesAtOrAbout" minOccurs="0"/>
			<xs:element ref="OffenseLocationText" minOccurs="0"/>
			<xs:element ref="OffenseOwnerText" minOccurs="0"/>
			<xs:element ref="OffenseTypeText" minOccurs="0"/>
			<xs:element ref="SeverityLevelText"/>
			<xs:element ref="GeneralOffenseCodeText" minOccurs="0"/>
			<xs:element ref="MinnesotaOffenseCode" minOccurs="0"/>
			<xs:element ref="ChargeLegalDescriptionText"/>
			<xs:element ref="PredatoryOffenderRegistrationRequiredIndicator" minOccurs="0"/>
			<xs:element ref="TargetedMisdemeanorIndicator" minOccurs="0"/>
			<xs:element name="ChargeStatuteSupplemental" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Statute that provides additional qualifications to the charge.   OffenseID values are restricted to those that are Minnesota state statutes.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="StatuteTypeText"/>
						<xs:element ref="OffenseID"/>
						<xs:element ref="OffenseCite" minOccurs="0"/>
						<xs:element ref="OffenseDescriptionText" minOccurs="0"/>
						<xs:element ref="SeverityLevelText" minOccurs="0"/>
						<xs:element ref="ChargeLegalDescriptionText"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Arrest" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="ArrestDate" minOccurs="0"/>
						<xs:element ref="ArrestTime" minOccurs="0"/>
						<xs:element ref="ArrestingAgencyORI" minOccurs="0"/>
						<xs:element ref="ArrestingAgencyControlNumber" minOccurs="0"/>
						<xs:element ref="ArrestingOfficial" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Report" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="ReportingAgencyORI" minOccurs="0"/>
						<xs:element ref="ReportingAgencyControlNumber" minOccurs="0"/>
						<xs:element ref="ReportingOfficial" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element ref="ProsecutingAgencyORI" minOccurs="0"/>
			<xs:element ref="CommunityOfOffenseText" minOccurs="0"/>
			<xs:element name="Vehicle" type="CaseGetVehicleType" minOccurs="0"/>
			<xs:element ref="AccidentDamageText" minOccurs="0"/>
			<xs:element ref="Speeding" minOccurs="0"/>
			<xs:element ref="AnimalsOverLimitCount" minOccurs="0"/>
			<xs:element ref="VehicleOverweightMeasure" minOccurs="0"/>
			<xs:element ref="BloodAlcoholConcentration" minOccurs="0"/>
			<xs:element ref="ReportToDPSIndicator" minOccurs="0"/>
			<xs:element ref="ReportToDNRIndicator" minOccurs="0"/>
			<xs:element ref="ChargeSubjectReference"/>
			<xs:element ref="ChargePlea" minOccurs="0"/>
			<xs:element ref="ChargeAmended" minOccurs="0"/>
			<xs:element ref="TrialDemand" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="chargeKey" type="xs:positiveInteger" use="required"/>
		<xs:attribute name="chargeVersionKey" type="xs:positiveInteger" use="required"/>
	</xs:complexType>
	<xs:complexType name="CaseGetChargeDispositionType">
		<xs:sequence>
			<xs:element name="Charge" type="CaseGetChargeType"/>
			<xs:element name="DispositionEvent" type="CaseGetDispositionEventType"/>
			<xs:element name="CaseDefendant" type="CaseDefendantType"/>
		</xs:sequence>
		<xs:attribute name="chargeKey" type="xs:positiveInteger" use="required"/>
	</xs:complexType>
	<xs:complexType name="CaseGetChargeAdjudicationType">
		<xs:sequence>
			<xs:element name="Charge" type="CaseGetChargeType"/>
			<xs:element name="DispositionEvent" type="CaseGetDispositionEventType"/>
			<xs:element name="ChildSubject">
				<xs:complexType>
					<xs:choice>
						<xs:element name="ChildDelinquent" type="ChildDelinquentType"/>
						<xs:element name="ChildNonDelinquent" type="ChildNonDelinquentType"/>
					</xs:choice>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="chargeKey" type="xs:positiveInteger" use="required"/>
	</xs:complexType>
	<xs:complexType name="CaseGetDispositionEventType">
		<xs:sequence>
			<xs:element ref="ChargeDispositionDate"/>
			<xs:element ref="ChargeDispositionJudge" minOccurs="0"/>
			<xs:element ref="ChargeDispositionText"/>
			<xs:element ref="ChargeDispositionSeverityLevel"/>
			<xs:element ref="ChargeDispositionAmendedReasonText" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="ChargeDispositionComment" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CaseGetCitationChargeType">
		<xs:sequence>
			<xs:element ref="ChargeSequenceID"/>
			<xs:element ref="OffenseID"/>
			<xs:element ref="OffenseCite"/>
			<xs:element ref="OffenseDescriptionText"/>
			<xs:element ref="OffenseOwnerText" minOccurs="0"/>
			<xs:element ref="OffenseTypeText" minOccurs="0"/>
			<xs:element ref="ThirdViolationIndicator" minOccurs="0"/>
			<xs:element ref="SeverityLevelText"/>
			<xs:element ref="GeneralOffenseCodeText" minOccurs="0"/>
			<xs:element ref="MinnesotaOffenseCode" minOccurs="0"/>
			<xs:element ref="ChargeLegalDescriptionText"/>
			<xs:element ref="PredatoryOffenderRegistrationRequiredIndicator" minOccurs="0"/>
			<xs:element ref="TargetedMisdemeanorIndicator" minOccurs="0"/>
			<xs:element ref="Speeding" minOccurs="0"/>
			<xs:element ref="AnimalsOverLimitCount" minOccurs="0"/>
			<xs:element ref="VehicleOverweightMeasure" minOccurs="0"/>
			<xs:element ref="BloodAlcoholConcentration" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="chargeKey" type="xs:positiveInteger" use="required"/>
	</xs:complexType>
	<xs:complexType name="CaseGetCitationDocumentType">
		<xs:sequence>
			<xs:element name="Filing">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="ProsecutingAgencyORI" minOccurs="0"/>
						<xs:element ref="CourtJurisdictionORI"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Citation">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="CitationNumber"/>
						<xs:element ref="OffenseDate"/>
						<xs:element ref="OffenseTime" minOccurs="0"/>
						<xs:element ref="TicketDate"/>
						<xs:element ref="IssuingAgencyORI"/>
						<xs:element ref="IssuingMethodText" minOccurs="0"/>
						<xs:element name="Incident">
							<xs:complexType>
								<xs:sequence>
									<xs:element ref="WeatherConditionText" minOccurs="0"/>
									<xs:element ref="PropertyDamageText" minOccurs="0"/>
									<xs:element ref="AccidentDamageText" minOccurs="0"/>
									<xs:element ref="OffenseLocationText" minOccurs="0"/>
									<xs:element ref="OfficerBadgeNumber" minOccurs="0"/>
									<xs:element ref="OfficerNameText" minOccurs="0"/>
									<xs:element ref="ReportingAgencyControlNumber" minOccurs="0"/>
									<xs:element ref="CommunityOfOffenseText" minOccurs="0"/>
									<xs:element ref="AppearBy" minOccurs="0"/>
									<xs:element name="Vehicle" type="CaseGetVehicleType" minOccurs="0"/>
									<xs:element ref="ParkingSpace" minOccurs="0"/>
									<xs:element ref="OfficerCommentText" minOccurs="0"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="Citee">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="PartyKey" type="IntegrationIDType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>The party integration ID. If provided will skip all other party matching logic and select the party with this id.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="CiteeInvolvementText" type="CiteeInvolvementTypeCodeTextType" nillable="true">
										<xs:annotation>
											<xs:documentation>Must be a value from the enumerated list.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="PartyName" type="PartyNameBaseType"/>
									<xs:element name="PartyAddress" type="PartyAddressBaseType" minOccurs="0"/>
									<xs:element name="PersonBirthDate" type="xs:date" minOccurs="0"/>
									<xs:element name="PersonAssignedIDDetails" minOccurs="0">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="PersonID" type="PersonIDType" minOccurs="0"/>
												<xs:element name="PersonDriverLicenseID" minOccurs="0" maxOccurs="unbounded">
													<xs:complexType>
														<xs:complexContent>
															<xs:extension base="PersonDriverLicenseIDType"/>
														</xs:complexContent>
													</xs:complexType>
												</xs:element>
												<xs:element name="PersonStateID" minOccurs="0" maxOccurs="unbounded">
													<xs:annotation>
														<xs:documentation>A number issued by a state Automated Fingerprint Identification System (AFIS) based on submitted fingerprints. Sometimes referred to as a State ID number or a SID.</xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:complexContent>
															<xs:extension base="PersonStateIDType"/>
														</xs:complexContent>
													</xs:complexType>
												</xs:element>
												<xs:element name="PersonOtherID" type="PersonOtherIDType" minOccurs="0" maxOccurs="unbounded"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="PersonPhysicalDetails" type="PersonPhysicalDetailsType" minOccurs="0"/>
									<xs:element name="FingerPrintedIndicator" type="xs:boolean">
										<xs:annotation>
											<xs:documentation>If true, indicates the defendant has been fingerprinted for this case. </xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="InCustodyIndicator" type="xs:boolean" minOccurs="0">
										<xs:annotation>
											<xs:documentation>If true, indicates the citee is in custody. If the flag is omitted, is has the same effect as false. </xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="IdentificationMethodText" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Method used to identify the citee.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="DL"/>
												<xs:enumeration value="DVS Web"/>
												<xs:enumeration value="Photo ID"/>
												<xs:enumeration value="FP"/>
												<xs:enumeration value="Other"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Charge" type="CaseGetCitationChargeType" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="citationKey" type="xs:positiveInteger" use="required"/>
	</xs:complexType>
	<xs:complexType name="CaseGetSentenceDispositionType">
		<xs:complexContent>
			<xs:extension base="CaseGetSentenceDelinquencyDispositionEventType">
				<xs:sequence>
					<xs:element name="Charges">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="ChargeDisposition" type="CaseGetChargeDispositionType" maxOccurs="unbounded"/>
								<xs:element ref="SentenceChargeSeverityLevel" minOccurs="0"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="SentenceDispositionDetail" minOccurs="0" maxOccurs="unbounded">
						<xs:complexType>
							<xs:complexContent>
								<xs:extension base="DispositionComponentType">
									<xs:sequence>
										<xs:element name="DispositionConditionElement" type="DispositionConditionElementType" minOccurs="0" maxOccurs="unbounded"/>
										<xs:element name="DispositionComponentStatus" type="StatusType" minOccurs="0"/>
									</xs:sequence>
								</xs:extension>
							</xs:complexContent>
						</xs:complexType>
					</xs:element>
					<xs:element name="SentenceEventHistoryReference" minOccurs="0">
						<xs:complexType>
							<xs:attribute name="sentenceEventKey"/>
						</xs:complexType>
					</xs:element>
					<xs:element name="SentenceHistoryReference" minOccurs="0">
						<xs:annotation>
							<xs:documentation>On an amended sentence this element's Sentence ID attribute points back to the Sentence ID of the sentence that was amended.  Using these references you can "walk" back to the original sentence, i.e., the Sentence element instance with no SentenceHistoryReferenceID element. </xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:attribute name="sentenceDelinquencyDispositionKey" type="xs:positiveInteger" use="required"/>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="sentenceDelinquencyDispositionKey" type="xs:positiveInteger" use="required">
					<xs:annotation>
						<xs:documentation>References a set of detailed information about the sentence.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="sentenceEventKey" type="xs:positiveInteger" use="required">
					<xs:annotation>
						<xs:documentation>References a set of general information about the sentence.  Case processing practices determine whether a sentenceEventKey exists  for each sentenceDelinquencyDispositionKey  OR if a sentenceEventKey is associated with multiple sentenceDelinquencyDispositionKey’s. May be deprecated in the future, for best results use the sentenceDelinquencyDispositionKey.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CaseGetSentenceAdjudicationType">
		<xs:complexContent>
			<xs:extension base="CaseGetSentenceDelinquencyDispositionEventType">
				<xs:sequence>
					<xs:element name="Charges">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="ChargeAdjudication" type="CaseGetChargeAdjudicationType" maxOccurs="unbounded"/>
								<xs:element ref="SentenceChargeSeverityLevel" minOccurs="0"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="SentenceDispositionDetail" minOccurs="0" maxOccurs="unbounded">
						<xs:complexType>
							<xs:complexContent>
								<xs:extension base="DispositionComponentType">
									<xs:sequence>
										<xs:element name="DispositionConditionSet" minOccurs="0" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="DispositionConditionElement" maxOccurs="unbounded">
														<xs:complexType>
															<xs:complexContent>
																<xs:extension base="DispositionConditionElementType"/>
															</xs:complexContent>
														</xs:complexType>
													</xs:element>
													<xs:element name="RelatedParty" type="RelatedPartyType" minOccurs="0" maxOccurs="unbounded"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="DispositionComponentStatus" type="StatusType" minOccurs="0"/>
									</xs:sequence>
								</xs:extension>
							</xs:complexContent>
						</xs:complexType>
					</xs:element>
					<xs:element name="SentenceEventHistoryReference" minOccurs="0">
						<xs:complexType>
							<xs:attribute name="sentenceEventKey"/>
						</xs:complexType>
					</xs:element>
					<xs:element name="SentenceHistoryReference" minOccurs="0">
						<xs:annotation>
							<xs:documentation>On an amended sentence this element's Sentence ID attribute points back to the Sentence ID of the sentence that was amended.  Using these references you can "walk" back to the original sentence, i.e., the Sentence element instance with no SentenceHistoryReferenceID element. </xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:attribute name="sentenceDelinquencyDispositionKey" type="xs:positiveInteger" use="required"/>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="sentenceDelinquencyDispositionKey" type="xs:positiveInteger" use="required">
					<xs:annotation>
						<xs:documentation>References a set of detailed information about the sentence.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="sentenceEventKey" type="xs:positiveInteger" use="required">
					<xs:annotation>
						<xs:documentation>References a set of general information about the sentence.  Case processing practices determine whether a sentenceEventKey exists  for each sentenceDelinquencyDispositionKey  OR if a sentenceEventKey is associated with multiple sentenceDelinquencyDispositionKey’s. May be deprecated in the future, for best results use the sentenceDelinquencyDispositionKey.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CaseGetSentenceDelinquencyDispositionEventType">
		<xs:annotation>
			<xs:documentation>Adult Sentencing or Juvenile Disposition Hearing</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="PronouncedDate"/>
			<xs:element ref="SentenceEventText"/>
			<xs:element ref="PronouncingJudge" minOccurs="0"/>
			<xs:element name="AmendedReasonText" type="AmendmentReasonSentenceTypeCodeTextType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CaseGetVehicleType">
		<xs:sequence>
			<xs:element ref="VehicleTypeText" minOccurs="0"/>
			<xs:element ref="VehicleMakeText" minOccurs="0"/>
			<xs:element ref="VehicleModelText" minOccurs="0"/>
			<xs:element ref="VehicleYear" minOccurs="0"/>
			<xs:element ref="VehicleColorText" minOccurs="0"/>
			<xs:element ref="VehicleLicensePlate"/>
			<xs:element ref="CommercialVehicleIndicator" minOccurs="0"/>
			<xs:element ref="HazardousMaterialsIndicator" minOccurs="0"/>
			<xs:element ref="VehicleID" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CaseOptionsType">
		<xs:all>
			<xs:element name="CourtCaseHeaderGroup" minOccurs="0">
				<xs:complexType>
					<xs:all>
						<xs:element name="SelectedIndicator" type="xs:boolean" default="false" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Include this element.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:all>
				</xs:complexType>
			</xs:element>
			<xs:element name="CourtCaseEvent" minOccurs="0">
				<xs:complexType>
					<xs:all>
						<xs:element name="SelectedIndicator" type="xs:boolean" default="false" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Include this element.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:all>
				</xs:complexType>
			</xs:element>
			<xs:element name="CasePartyRelation" minOccurs="0">
				<xs:complexType>
					<xs:all>
						<xs:element name="SelectedIndicator" type="xs:boolean" default="false" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Include this element.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:all>
				</xs:complexType>
			</xs:element>
			<xs:element name="Attorney" minOccurs="0">
				<xs:complexType>
					<xs:all>
						<xs:element name="SelectedIndicator" type="xs:boolean" default="false" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Include this element.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:all>
				</xs:complexType>
			</xs:element>
			<xs:element name="CaseProsecutor" minOccurs="0">
				<xs:complexType>
					<xs:all>
						<xs:element name="SelectedIndicator" type="xs:boolean" default="false" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Include this element.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:all>
				</xs:complexType>
			</xs:element>
